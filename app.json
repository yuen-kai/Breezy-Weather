{
  "expo": {
    "name": "Breezy",
    "slug": "newSimpleWeather",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconIOSlight.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": {
        "light": "./assets/images/iconIOSlight.png",
        "dark": "./assets/images/iconIOSdark.png",
        "tinted": "./assets/images/iconIOStint.png"
      },
      "bundleIdentifier": "com.yuenkai.breezy",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We use your location to show the weather for your current area.",
        "NSLocationWhenInUseUsageDescription": "We use your location to show the weather for your current area.",
        "NSLocationAlwaysUsageDescription": "We use your location to show the weather for your current area."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png"
      },
      "package": "com.yuenkai.simpleWeather"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cf412d15-4ca5-472c-99d7-ae8264b6dffc"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/cf412d15-4ca5-472c-99d7-ae8264b6dffc",
      "checkAutomatically": "NEVER"
    }
  }
}
